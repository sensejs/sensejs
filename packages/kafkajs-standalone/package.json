    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "kafkajs": "^1.16.0 || ^2.0.2"
  },
  "devDependencies": {
    "@sensejs/testing-utility": "workspace:*",
    "config": "^3.3.7"
  }
