  "license": "ISC",
  "dependencies": {
    "@sensejs/utility": "workspace:0.10.0-alpha.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@sensejs/container": "workspace:*",
    "@sensejs/testing-utility": "workspace:*",
    "@types/uuid": "^8.3.4"
  },
  "peerDependencies": {
    "@sensejs/container": "workspace:^0.10.0-alpha.0",
    "reflect-metadata": "^0.1.13"
  }
