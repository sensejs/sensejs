{
  "name": "@sensejs/kafkajs-zstd-support",
  "version": "0.10.0-alpha.0",
  "description": "",
  "files": [
    "dist-cjs/**/!(*.tsbuildinfo)",
    "dist-esm/**/!(*.tsbuildinfo)",
    "src/**/*.ts",
    "README.md"
  ],
  "type": "module",
  "types": "./dist-esm/lib/index.d.ts",
  "main": "./dist-cjs/lib/index.js",
