{
  "name": "@sensejs/http-koa-platform",
  "version": "0.9.0",
  "description": "",
  "files": [
    "dist-cjs/**/!(*.tsbuildinfo)",
    "dist-esm/**/!(*.tsbuildinfo)",
    "src/**/*.ts",
    "README.md"
  ],
  "type": "module",
  "types": "./dist-esm/lib/index.d.ts",
  "main": "./dist-cjs/lib/index.js",
  "module": "./dist-esm/lib/index.js",
  "exports": {
    "import": "./dist-esm/lib/index.js",
    "require": "./dist-cjs/lib/index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.build-cjs.json && tsc -p tsconfig.build-esm.json",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rm -rf ./dist-esm/lib dist-cjs/lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@sensejs/utility": "workspace:^0.9.0-alpha.6",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-qs": "^3.0.0",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@sensejs/container": "workspace:*",
    "@sensejs/core": "workspace:*",
    "@sensejs/http-common": "workspace:*",
    "@sensejs/testing-utility": "workspace:*"
  },
  "peerDependencies": {
    "@sensejs/container": "workspace:^0.9.0-alpha.4",
    "@sensejs/core": "workspace:^0.9.0-alpha.5",
    "@sensejs/http-common": "workspace:^0.9.0-alpha.2",
    "reflect-metadata": "^0.1.13"
  },
  "author": "LAN Xingcan",
  "license": "ISC"
}
