{
  "name": "@sensejs/kafkajs",
  "version": "0.10.0",
  "description": "",
  "files": [
    "dist-cjs/**/!(*.tsbuildinfo)",
    "dist-esm/**/!(*.tsbuildinfo)",
    "dist-types/**",
    "src/**/*.ts",
    "README.md"
  ],
  "type": "module",
  "types": "./dist-types/index.d.ts",
  "main": "./dist-cjs/lib/index.js",
  "module": "./dist-esm/lib/index.js",
  "exports": {
    "types": "./dist-types/index.d.ts",
    "import": "./dist-esm/lib/index.js",
    "require": "./dist-cjs/lib/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build-types.json && tsc -p tsconfig.build-cjs.json && tsc -p tsconfig.build-esm.json",
    "clean": "rm -rf ./dist-esm/lib dist-cjs/lib dist-types",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "LAN Xingcan",
  "license": "ISC",
  "dependencies": {
    "@sensejs/kafkajs-standalone": "workspace:^0.10.0",
    "@sensejs/utility": "workspace:^0.10.0",
    "lodash": "^4.17.21",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@sensejs/container": "workspace:*",
    "@sensejs/core": "workspace:*",
    "@sensejs/testing-utility": "workspace:*",
    "config": "^3.3.9"
  },
  "peerDependencies": {
    "@sensejs/container": "workspace:^0.10.0",
    "@sensejs/core": "workspace:^0.10.0",
    "kafkajs": "^1.16.0 || ^2.0.2",
    "reflect-metadata": "^0.1.13"
  }
}
